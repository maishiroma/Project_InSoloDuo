        x += sign(hsp);
    }
    hsp = 0;
    dir *= -1;
}
else if(place_meeting(x+hsp,y,obj_invisibleGimmickWall)){
    while(!place_meeting(x+sign(hsp),y,obj_invisibleGimmickWall)){
        x += sign(hsp);
    }    
    hsp = 0;
    dir *= -1;
}
x += hsp;

//One way platform physics
if(instance_exists(SuperClass_Player)){
    if (round(SuperClass_Player.y + (SuperClass_Player.sprite_height/2)) &gt; y) || (SuperClass_Player.key_down ){
        mask_index = -1;
    }
    else{
        mask_index = spr_MovingPlatform;
        if(place_meeting(x,y-1,SuperClass_Player)){
            SuperClass_Player.hsp_carry = hsp;
        }
    }
}

if(canReset == true){
    if(global.active == true){
        x = xstart;
        y = ystart;
    }
}
</string>
